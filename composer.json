{
  "name": "sonrac/yii2-relation-trait",
  "require": {
    "php": ">=7",
    "yiisoft/yii2": "2.*",
    "unclead/yii2-multiple-input": "2.*",
    "yiisoft/yii2-bootstrap": "^2.0"
  },
  "require-dev": {
    "codeception/base": "2.*",
    "codeception/verify": "~0.3.3",
    "codeception/specify": "~0.4.6",
    "codeception/codeception": "^2.2.10"
  },
  "authors": [
    {
      "name": "Doniy Serhey",
      "email": "doniysa@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "sonrac\\relations\\": "src/"
    }
  },

  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    },
    "asset-vcs-driver-options": {
      "github-no-api": true
    },
    "asset-registry-options": {
      "npm-searchable": false,
      "bower-searchable": false
    },
    "asset-pattern-skip-version": "(-build|-patch)"
  }
}
